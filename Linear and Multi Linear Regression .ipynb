    "class LinearRegression():\n",
    "    def fit(self, X,y):\n",
    "        self.w=np.linalg.solve(X.T@X, X.T@y)\n",
    "        \n",
    "    def predict(self, X):\n",
    "        \n",
    "        return np.matmul(X, self.w)"
   ]
